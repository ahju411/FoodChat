<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="95dp"
        android:layout_height="95dp"
        app:layout_constraintVertical_bias="0.1"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </ImageView>

    <Button
        android:id="@+id/store_register"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:background="@drawable/btn_darknavi"
        android:text="식당등록"
        android:textColor="@color/white"
        app:layout_constraintVertical_bias="0.35"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/manage_review"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:background="@drawable/btn_darknavi"
        android:text="리뷰관리"
        android:textColor="@color/white"
        android:layout_marginTop="60dp"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf ="@id/store_register" />

    <Button
        android:id="@+id/manage_reservation"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:background="@drawable/btn_darknavi"
        android:text="예약관리"
        android:textColor="@color/white"
        android:layout_marginTop="60dp"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/manage_review" />

    <Button
        android:id="@+id/manage_fa"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:background="@drawable/btn_darknavi"
        android:text="F&amp;A관리"
        android:textColor="@color/white"
        android:layout_marginTop="60dp"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/manage_reservation"/>



</androidx.constraintlayout.widget.ConstraintLayout>